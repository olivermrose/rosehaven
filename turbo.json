{
	"tasks": {
		"lint": {},
		"sync": {
			"cache": false
		},
		"build": {
			"inputs": [
				"$TURBO_DEFAULT$",
				".env"
			],
			"env": [
				"PAYLOAD_SECRET",
				"DATABASE_URL"
			],
			"dependsOn": [
				"^build"
			],
			"outputs": [
				".next/**",
				"!.next/cache/**",
				".svelte-kit/**",
				".vercel/**"
			]
		},
		"dev": {
			"inputs": [
				"$TURBO_DEFAULT$",
				".env"
			],
			"cache": false,
			"persistent": true
		}
	}
}
